{
	"name": "@qt-core/core",
	"version": "1.0.0",
	"license": "MIT",
	"main": "index.ts",
	"scripts": {
		"compile": "hardhat compile",
		"build": "npm run compile && npm run build:types",
		"build:types": "cross-env HARDHAT_WEB3_TYPE=1 hardhat typechain",
		"build:test-types": "hardhat typechain",
		"deploy": "hardhat run",
		"deploy:local": "hardhat run --network localhost scripts/fork/deploy.ts",
		"fork:run": "hardhat run --network localhost",
		"verify": "hardhat verify --network goerli",
		"test": "npm run test:basic && npm run test:fork",
		"test:basic": "hardhat test test/TradePool.spec.ts",
		"test:fork": "cross-env HARDHAT_FORK_TEST=1 hardhat test test/fork/*.spec.ts",
		"node:local": "cross-env HARDHAT_FORK_TEST=1 hardhat node",
		"watch": "remixd -s . --remix-ide https://remix.ethereum.org",
		"clean": "rm -rf .deps && rm -rf artifacts && rm -rf cache && rm -rf web3-types && rm -rf test-types"
	},
	"devDependencies": {
		"@ethersproject/abi": "^5.7.0",
		"@ethersproject/providers": "^5.7.0",
		"@nomicfoundation/hardhat-chai-matchers": "^1.0.3",
		"@nomicfoundation/hardhat-network-helpers": "^1.0.4",
		"@nomicfoundation/hardhat-toolbox": "^1.0.2",
		"@nomiclabs/hardhat-ethers": "^2.1.1",
		"@nomiclabs/hardhat-etherscan": "^3.1.0",
		"@openzeppelin/hardhat-upgrades": "^1.21.0",
		"@remix-project/remixd": "^0.6.5",
		"@typechain/ethers-v5": "^10.1.0",
		"@typechain/hardhat": "^6.1.2",
		"@typechain/web3-v1": "^6.0.1",
		"@types/chai": "^4.3.3",
		"@types/mocha": "^9.1.1",
		"@types/node": "^18.7.13",
		"chai": "^4.3.6",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.1",
		"ethers": "^5.7.0",
		"hardhat": "^2.10.2",
		"hardhat-gas-reporter": "^1.0.8",
		"prettier-plugin-solidity": "^1.0.0-beta.24",
		"solidity-coverage": "^0.7.21",
		"ts-node": "^10.9.1",
		"typechain": "^8.1.0",
		"typescript": "^4.8.2"
	},
	"dependencies": {
		"@openzeppelin/contracts": "^4.8.0",
		"@openzeppelin/contracts-upgradeable": "^4.8.0",
		"@uniswap/v3-periphery": "^1.4.3"
	}
}
